package Main;

import DAO.QuizDAO;
import DTO.QuizDTO;

public class DAOTEST {

	QuizDTO dto = new QuizDTO();
	QuizDAO dao = new QuizDAO();
	
	int num = dao.getQuizCount();
	int randomNum = (int) (Math.random() * num);
	
	public static void main(String[] args) {
		DAOTEST dt = new DAOTEST();
		System.out.println("dao test num :: " + dt.num);
		System.out.println("dao test random num :: " + dt.randomNum);
	}
}
